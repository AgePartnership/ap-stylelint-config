{
  "name": "@agepartnership/ap-stylelint-config",
  "version": "2.0.1",
  "description": "StyleLint Configuration for AgePartnership's SCSS",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:AgePartnership/ap-stylelint-config.git"
  },
  "publishConfig": {
